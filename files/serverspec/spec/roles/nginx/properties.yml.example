localhost:
  :roles:
    - nginx
  :vars:
    :nginx_version: 1.6.3
	:nginx_user: nginx
	:nginx_group: nginx
	:nginx_log_dir: /mnt/logs/nginx
	:nginx_install_dir: /etc/nginx
	:nginx_log_format: $remote_addr - $remote_user [$time_local] "$request" $status $body_bytes_sent "$http_referer" "$http_user_agent" "$http_x_forwarded_for"